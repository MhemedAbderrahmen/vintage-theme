{
  "name": "Aggressive Retro",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editor.selectionBackground": "#ff6600",
    "editor.lineHighlightBackground": "#333333",
    "editorCursor.foreground": "#ffffff",
    "editorWhitespace.foreground": "#444444",
    "editorIndentGuide.background": "#444444",
    "editorLineNumber.foreground": "#808080",
    "editorRuler.foreground": "#666666"
  },
  "tokenColors": [
    {
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#ff6666"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#66ff66"
      }
    },
    {
      "scope": ["support.function", "support.class", "support.type"],
      "settings": {
        "foreground": "#66b3ff"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic"
      }
    }
  ],
  "semanticTokenColors": {
    "functions": "#66b3ff",
    "variables": "#ffcc00",
    "constants": "#66ff66",
    "keywords": "#ff6666",
    "comments": "#999999"
  }
}
