{
  "name": "Vintage Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#2b2b2b",
    "editor.foreground": "#e0c88c",
    "editorLineNumber.foreground": "#907020",
    "editorCursor.foreground": "#c6a668",
    "editor.selectionBackground": "#5a4b2a",
    "editor.selectionHighlightBackground": "#7c5e3c",
    "activityBar.background": "#2b2b2b",
    "sideBar.background": "#302b2b",
    "sideBar.foreground": "#d4b684",
    "statusBar.background": "#2b2b2b",
    "statusBar.foreground": "#d4b684",
    "titleBar.activeBackground": "#2b2b2b",
    "titleBar.activeForeground": "#d4b684",
    "titleBar.inactiveBackground": "#302b2b",
    "titleBar.inactiveForeground": "#b3a06e"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#907020",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#d1b37c"
      }
    },
    {
      "scope": ["variable", "parameter", "property", "function"],
      "settings": {
        "foreground": "#e0c88c"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#d4b684",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant", "entity.name.constant"],
      "settings": {
        "foreground": "#c6a668"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#c8b071",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["support.class.component", "entity.name.tag.jsx"],
      "settings": {
        "foreground": "#e6be85"
      }
    },
    {
      "scope": ["meta.tag.jsx", "meta.tag.js"],
      "settings": {
        "foreground": "#d4b684"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag.begin.jsx",
        "punctuation.definition.tag.end.jsx"
      ],
      "settings": {
        "foreground": "#c8a066"
      }
    },
    {
      "scope": ["entity.other.attribute-name.jsx"],
      "settings": {
        "foreground": "#d4b684",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.function.react", "support.function.react-hooks"],
      "settings": {
        "foreground": "#e6be85",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.embedded.expression.jsx"],
      "settings": {
        "foreground": "#c8b071"
      }
    }
  ]
}
